		$.ajax({
			url : $("#appForm").attr('action'),
			type : 'POST',
			data : $('#appForm').serialize(),
			success : function(data) {
				alert(data);
			},
			error: function(xhr, status, error) {
				var err = eval("(" + xhr.responseText + ")");
				  alert(err.Message);
				  alert(status);
				}
		});
		return false;
	}
