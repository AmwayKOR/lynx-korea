<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <beans xmlns="http://www.springframework.org/schema/beans"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


        <alias alias="amwayGroupProfileService" name="defaultAmwayGroupProfileService" />
        <bean id="defaultAmwayGroupProfileService"
              class="com.amway.amwaydms.aboinformation.services.impl.DefaultAmwayGroupProfileService"
              parent="amwayAbstractDmsService">
            <property name="outputConverter" ref="amwayAccountResponseConverter" />
            <property name="urlPath"
                      value="${amwaydms.dms.url}" />
        </bean>

        <bean id="mockAmwayGroupProfileService" class="com.amway.amwaydms.aboinformation.services.impl.mock.MockAmwayProfileService">
            <property name="outputConverter" ref="amwayAccountResponseConverter" />
            <property name="urlPath"
                      value="${amwaydms.dms.url}" />
        </bean>


    </beans>

</beans>