<?xml version="1.0" encoding="ISO-8859-1"?>
<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:noNamespaceSchemaLocation="items.xsd">
	<maptypes>
		<maptype code="AmwayEventMetaInfo" argumenttype="java.lang.String" returntype="java.lang.String" />
	</maptypes>

	<itemtypes>
		<itemtype code="AmwayEventQueueEntry" autocreate="true" generate="true">
			<description>The item is used to temporary store events which are published for
				future consumption by external systems.
			</description>
			<deployment table="AmwayEventQueueEntry" typecode="25018"/>
			<attributes>
				<attribute qualifier="code" type="java.lang.String">
					<description>Event generated code</description>
					<modifiers unique="true" optional="false" />
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="name" type="java.lang.String">
					<description>Name of the event. Examples: OrderPlaced, CreditCardAdded, etc.</description>
					<modifiers unique="false" optional="false" />
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="generationTime" type="java.util.Date">
					<description>Date and time of event generation with millisecond precision.</description>
					<modifiers unique="false" optional="true" />
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="entityID" type="java.lang.String">
					<description>Related item id.</description>
					<modifiers unique="false" optional="false" />
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="entityName" type="java.lang.String">
					<description>Name of the entity the event is about. Example: Order.</description>
					<modifiers unique="false" optional="false" />
					<persistence type="property"/>
				</attribute>

				<attribute qualifier="retrievalTime" type="java.util.Date">
					<description>Indicates if the event has already been retrieved (by webMethods). Value is null if it hasn't yet, an actual date otherwise.</description>
					<modifiers unique="false" optional="true" />
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="triggeredBy" type="java.lang.String">
					<description>Tells what triggered the current event. Possible values: Customer, CSA, Cron.</description>
					<modifiers unique="false" optional="false" />
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="accountID" type="java.lang.String">
					<description>Identifier of current account when event was generated, i.e AmwayAccount.code. May be null if generated by a cron job.</description>
					<modifiers unique="false" optional="true" />
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="countryCode" type="java.lang.String">
					<description>ISO code of the country where even was generated.</description>
					<modifiers unique="false" optional="false" />
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="targetSystem" type="java.lang.String">
					<description>Code of the target system.</description>
					<modifiers unique="false" optional="false" />
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="meta" type="AmwayEventMetaInfo">
					<description>Additional event information.</description>
					<persistence type="property"/>
				</attribute>
			</attributes>
		</itemtype>
	</itemtypes>
</items>
